-- üå∏ Kuni Hub X GUI (Rayfield)
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create Window
local Window = Rayfield:CreateWindow({
    Name = "Kuni Hub X",            -- Title
    LoadingTitle = "Kuni Hub X",    -- Loading title
    LoadingSubtitle = "by Kuni"     -- Updated subtitle/credit
})

-- Main Tab
local MainTab = Window:CreateTab("Main")
MainTab:CreateSection("Utilities")

-- Buttons
local buttonList = {
    {Name = "‚è≥ Open Pet Age Changer", Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/KuniDevLua/GrowAGarden/refs/heads/main/PetAgeChanger"))()
    end},
    {Name = "üß¨ Open Pet Mutation Finder", Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/KuniDevLua/GrowAGarden/refs/heads/main/Mt"))()
    end},
    {Name = "üõ† Duplicator", Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/KuniDevLua/GrowAGarden/refs/heads/main/petduplicator"))()
    end}
}

for _, btn in ipairs(buttonList) do
    MainTab:CreateButton({
        Name = btn.Name,
        Callback = btn.Callback
    })
end
